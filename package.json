{
    "name": "ng4-seed",
    "version": "0.0.0",
    "description": "Angular4 Webpack Seed",
    "homepage": "https://github.com/amily4555/ng4-seed",
    "repository": {
        "type": "git",
        "url": "https://github.com/amily4555/ng4-seed"
    },
    "author": {
        "name": "Mizi Lin",
        "email": "amily4555@gmail.com"
    },
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=6.10"
    },
    "scripts": {
        "build": "cross-env NODE_ENV=prod webpack --display-chunks --progress",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
        "clean": "del-cli ./dist",
        "lint": "npm-run-all lint:js lint:ts",
        "lint:js": "eslint -c .eslintrc.yml *.js ./src ./server",
        "lint:ts": "tslint './src/**/*.ts'",
        "postinstall": "npm run typings",
        "prebuild": "npm-run-all clean lint:ts",
        "server": "cross-env NODE_ENV=dev nodemon -w 'server/**/*.*' ./server/main.js",
        "server:dev": "cross-env NODE_ENV=dev webpack-dev-server --progress",
        "server:test": "cross-env NODE_ENV=test webpack-dev-server --progress",
        "server:prod": "cross-env NODE_ENV=prod webpack-dev-server --progress",
        "test": "npm run server:test",
        "start": "npm run server:dev",
        "test:watch": "cross-env NODE_ENV=test karma start",
        "typings": "typings install",
        "version": "npm run changelog && git add CHANGELOG.md"
    },
    "dependencies": {
        "@angular/common": "4.0.0-rc.3",
        "@angular/compiler": "4.0.0-rc.3",
        "@angular/core": "4.0.0-rc.3",
        "@angular/forms": "4.0.0-rc.3",
        "@angular/http": "4.0.0-rc.3",
        "@angular/platform-browser": "4.0.0-rc.3",
        "@angular/platform-browser-dynamic": "4.0.0-rc.3",
        "@angular/router": "^4.0.0-rc.3",
        "core-js": "^2.4.0",
        "mzmu": "^1.8.4",
        "rxjs": "^5.2.0",
        "zone.js": "^0.8.0"
    },
    "devDependencies": {
        "angular2-router-loader": "^0.3.5",
        "angular2-template-loader": "^0.6.2",
        "autoprefixer": "~6.3.6",
        "awesome-typescript-loader": "^3.1.2",
        "babel-core": "^6.14.0",
        "babel-loader": "^6.2.5",
        "conventional-changelog-cli": "~1.2.0",
        "copy-webpack-plugin": "~3.0.1",
        "cross-env": "~1.0.8",
        "css-loader": "~0.23.1",
        "del-cli": "~0.2.0",
        "eslint": "~2.13.1",
        "express": "~4.14.0",
        "extract-text-webpack-plugin": "^2.1.0",
        "file-loader": "^0.9.0",
        "html-webpack-plugin": "~2.21.0",
        "jasmine-core": "~2.4.1",
        "karma": "~1.1.0",
        "karma-chrome-launcher": "~1.0.1",
        "karma-jasmine": "~1.0.2",
        "karma-sourcemap-loader": "~0.3.7",
        "karma-webpack": "^2.0.2",
        "morgan": "~1.7.0",
        "node-sass": "^4.5.0",
        "nodemon": "~1.9.2",
        "npm-run-all": "~2.3.0",
        "postcss-loader": "~0.9.1",
        "raw-loader": "^0.5.1",
        "sass": "^0.5.0",
        "sass-loader": "^6.0.3",
        "style-loader": "~0.13.1",
        "ts-loader": "^2.0.1",
        "tslint": "^4.5.1",
        "typescript": "^2.2.1",
        "typings": "^2.1.0",
        "url-loader": "^0.5.7",
        "web-animations-js": "^2.2.2",
        "webpack": "^2.2.1",
        "webpack-dev-server": "~1.14.1",
        "webpack-md5-hash": "~0.0.5",
        "winston": "~2.2.0"
    }
}
